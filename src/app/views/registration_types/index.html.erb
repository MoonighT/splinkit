<h1>Listing registration_types</h1>

<table>
  <tr>
    <th>Event</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @registration_types.each do |registration_type| %>
  <tr>
    <td><%= registration_type.event_id %></td>
    <td><%= registration_type.name %></td>
    <td><%= link_to 'Show', registration_type %></td>
    <td><%= link_to 'Edit', edit_registration_type_path(registration_type) %></td>
    <td><%= link_to 'Destroy', registration_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

